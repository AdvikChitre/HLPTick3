        <dependency id="System.ValueTuple" version="4.3.1" exclude="Build,Analyzers" />
      </group>
      <group targetFramework=".NETFramework4.5">
        <dependency id="FSharp.Core" version="3.0.2" exclude="Build,Analyzers" />
        <dependency id="System.ValueTuple" version="4.3.1" exclude="Build,Analyzers" />
      </group>
      <group targetFramework=".NETStandard2.0">
        <dependency id="FSharp.Core" version="4.2.3" exclude="Build,Analyzers" />
        <dependency id="System.Reflection.Emit" version="4.3.0" exclude="Build,Analyzers" />
      </group>
    </dependencies>
  </metadata>
</package>